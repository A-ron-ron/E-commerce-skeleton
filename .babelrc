{
    "ignore": ["/(node_modules)/"],
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-transform-runtime",
        "@babel/syntax-dynamic-import",
        "dynamic-import-node",
        "react-loadable/babel"
    ]
}